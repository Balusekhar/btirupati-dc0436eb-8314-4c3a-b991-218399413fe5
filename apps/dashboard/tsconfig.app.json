{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@org/data": [
        "../../data/src/index.ts"
      ],
      "@org/data/*": [
        "../../data/src/*"
      ]
    }
  },
  "include": [
